file = { SOI ~ sexp+ ~ EOI }
sexp = _{ atom | list  }
list = { "(" ~ (sexp)* ~ ")" }
atom = _{ number | symbol | string }
number = @{ ASCII_DIGIT+ }
string = @{ "\"" ~ (!"\"" ~ ANY)* ~ "\"" }
symbol = @{ sym_char+ }
sym_char = _{ ASCII_ALPHANUMERIC | ":" | "!" | "*" | "+" | "|" | "#" | "&" | "-" | "_" | "<" | ">" }
WHITESPACE = _{ "," | " " | "\t" | NEWLINE }
COMMENT = _{ ";;" ~ (!NEWLINE ~ ANY)* ~ NEWLINE }
